{
    "name": "docs",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy": "npm run build && wrangler pages deploy",
        "dev": "vite dev",
        "format": "prettier --write .",
        "generate-social": "tsx scripts/generate-social-images.ts",
        "lint": "prettier --check . && eslint .",
        "lint:fix": "npm run format && eslint . --fix",
        "preview": "vite preview",
        "sitemap:manifest": "node ./scripts/generate-sitemap-manifest.mjs",
        "sitemap:watch": "chokidar 'src/routes/**/*.svelte' 'src/routes/**/*.svx' 'src/routes/**/*.md' --ignore 'src/routes/examples/+page.ts' -c 'node ./scripts/generate-sitemap-manifest.mjs' --initial --silent",
        "test": "npm run test:unit -- --run && npm run test:e2e",
        "test:e2e": "playwright test",
        "test:unit": "vitest"
    },
    "dependencies": {
        "@humanspeak/svelte-headless-table": "workspace:*",
        "@humanspeak/svelte-motion": "^0.1.12",
        "github-slugger": "^2.0.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20251011.0",
        "@eslint/compat": "^1.4.0",
        "@eslint/js": "^9.37.0",
        "@faker-js/faker": "^10.0.0",
        "@internationalized/date": "^3.10.0",
        "@sveltejs/adapter-auto": "^6.1.1",
        "@sveltejs/adapter-cloudflare": "^7.2.4",
        "@sveltejs/kit": "^2.46.4",
        "@sveltejs/vite-plugin-svelte": "^6.2.1",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/postcss": "^4.1.14",
        "@tailwindcss/typography": "^0.5.19",
        "@tailwindcss/vite": "^4.1.14",
        "@tanstack/table-core": "^8.20.5",
        "@typescript-eslint/eslint-plugin": "^8.46.0",
        "@typescript-eslint/parser": "^8.46.0",
        "autoprefixer": "^10.4.21",
        "bits-ui": "^2.11.4",
        "chokidar-cli": "^3.0.0",
        "clsx": "^2.1.1",
        "embla-carousel-svelte": "^8.6.0",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-svelte": "3.12.4",
        "eslint-plugin-unused-imports": "4.2.0",
        "formsnap": "^2.0.1",
        "globals": "^16.4.0",
        "lucide-svelte": "^0.545.0",
        "mdsvex": "^0.12.6",
        "mode-watcher": "^1.1.0",
        "paneforge": "^1.0.2",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "prettier-plugin-sort-json": "^4.1.1",
        "prettier-plugin-svelte": "^3.4.0",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "puppeteer": "^24.23.0",
        "shiki": "^3.13.0",
        "svelte": "^5.39.11",
        "svelte-check": "^4.3.3",
        "svelte-sonner": "^1.0.5",
        "sveltekit-superforms": "^2.27.2",
        "tailwind-merge": "^3.3.1",
        "tailwind-variants": "^3.1.1",
        "tailwindcss": "^4.1.14",
        "tailwindcss-animate": "^1.0.7",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.0",
        "vaul-svelte": "^1.0.0-next.6",
        "vite": "^7.1.9",
        "vitest": "^3.2.4",
        "wrangler": "^4.42.2",
        "zod": "^4.1.12"
    },
    "volta": {
        "node": "22.14.0"
    }
}
